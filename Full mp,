for _, v in pairs(game:GetService("Workspace").GameObjects:GetChildren()) do
    if v:FindFirstChild("WaveTime") then
        v.WaveTime.Value = 1
    end
end

for _, v in pairs(game:GetService("Workspace").Spawners:GetChildren()) do
    if v:FindFirstChild("SpawnTime") then
        v.SpawnTime.Value = 0.1
    end
end
